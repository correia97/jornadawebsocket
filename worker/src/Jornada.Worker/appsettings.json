{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "sqs": {
    "arn": "arn:aws:sns:sa-east-1:000000000000:websocket-topic",
    "queueUrl": "https://localhost.localstack.cloud:4566/000000000000/websocket-queue"
  },
  "LocalStack": {
    "UseLocalStack": true,
    "Session": {
      "RegionName": "sa-east-1"
    },
    "Config": {
      "LocalStackHost": "localhost", // or "localhost",
      "EdgePort": 4566
    }
  },
  "AWS": {
    "Region": "sa-east-1"
  }
}
