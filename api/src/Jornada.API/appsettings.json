{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "sns": {
    "arn": "arn:aws:sns:sa-east-1:000000000000:websocket-topic"
  },
  "LocalStack": {
    "UseLocalStack": true,
    "Session": {
      "RegionName": "sa-east-1"
    },
    "Config": {
      "LocalStackHost": "localstack", // or "localhost",
      "EdgePort": 4566
    }
  },
  "AWS": {
    "Region": "sa-east-1"
  }
}
